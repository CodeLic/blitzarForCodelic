{
  "name": "@blitzar/form",
  "version": "1.1.2",
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "description": "Blitzar form component",
  "module": "./dist/index.es.js",
  "main": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint ./src/**.*",
    "gen:types:vetur": "vue-int -i src -r -o dist/vetur",
    "gen:types:ts": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "npm run lint && rimraf dist && vite build && npm run gen:types:ts && npm run gen:types:vetur",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "@blitzar/types": "^1.1.2",
    "@blitzar/utils": "^1.1.2",
    "case-anything": "^2.1.2",
    "copy-anything": "^2.0.3",
    "filter-anything": "^2.2.1",
    "is-what": "^3.14.1",
    "map-anything": "^1.0.4",
    "merge-anything": "^4.0.2",
    "nestify-anything": "^1.0.3",
    "snarkdown": "^2.0.0"
  },
  "keywords": [
    "blitzar",
    "blitz-form",
    "data-form",
    "quasar-easy-forms",
    "quasar-easy-form",
    "easy-forms",
    "easy-form",
    "vue-form",
    "form",
    "form-component",
    "form-builder",
    "form-generator",
    "schema-form",
    "vue"
  ],
  "license": "MIT",
  "homepage": "https://github.com/cycraft/blitzar#readme",
  "bugs": {
    "url": "https://github.com/cycraft/blitzar/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycraft/blitzar.git"
  },
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  },
  "gitHead": "5d30b79ccaa443d7db97757c0cfc89f822a1c36d"
}
